{
  "name": "pulse-node-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./dist-server/bin/www",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "dev": "cross-env NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "python-shell": "^3.0.1",
    "query-string": "^7.1.1",
    "rimraf": "^3.0.2",
    "ws": "^8.7.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
